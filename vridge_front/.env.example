# Environment Configuration
NODE_ENV=development

# API URLs
REACT_APP_BACKEND_URI=http://localhost:8000
REACT_APP_SOCKET_URI=ws://localhost:8000

# Social Login API Keys (Development)
REACT_APP_KAKAO_API_KEY=your-dev-kakao-api-key
REACT_APP_NAVER_CLIENT_ID=your-dev-naver-client-id
REACT_APP_NAVER_SECRET_KEY=your-dev-naver-secret
REACT_APP_GOOGLE_CLIENT_ID=your-dev-google-client-id

# Redirect URIs
REACT_APP_NAVER_REDIRECT_URI=http://localhost:3000/login

# Performance Settings
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
FAST_REFRESH=true